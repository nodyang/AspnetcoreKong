{
  "kong": {
    "adminApi": "http://10.0.1.222:8001",
    "service": {
      "name": "UsageService",
      "host": "UsageService",
      "routes": [
        {
          "name": "api",
          "paths": [
            "/api/xx"
          ],
          "strip_path": false
        }
      ]
    },
    "upStream": {
      "targets": [
        {
          "target": "{ip}:5000",
          "weight": 100
        }
      ],
      "healthchecks": {
        "active": {
          "http_path": "/healthchecks",
          "timeout": 5,
          "unhealthy": {
            "tcp_failures": 1,
            "timeouts": 1,
            "http_failures": 1,
            "interval": 10
          },
          "healthy": {
            "interval": 10,
            "successes": 1
          }
        }
      }
    }
  }
}